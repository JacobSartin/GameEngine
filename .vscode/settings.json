{
  "sops.enabled": false,
  "cmake.debugConfig": {
    "console": "integratedTerminal"
  },
  "cmake.exportCompileCommandsFile": true,
  "clangd.arguments": [
    "--compile-commands-dir=${workspaceFolder}/out/build/x64-debug",
    "--experimental-modules-support",
    "--log=verbose",
    "-j=1",
    "--background-index=0",
    "--clang-tidy=0"
  ],
  "clangd.fallbackFlags": ["-std=c++20"]
}
